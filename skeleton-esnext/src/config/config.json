{
  "name": "Test Application",
  "version": "1.2",
  "i18n": {
    "backend": {
      "loadPath": "/locales/{{lng}}/{{ns}}.json"
    },
    "lng" : "de",
    "attributes" : ["t","i18n"],
    "fallbackLng" : "en",
    "debug" : false,
    "namespaces": ["translation","nav"],
    "defaultNs": "translation"
  },
  "notification": {
    "translate": true,
    "notifications": {
      "success": "humane-libnotify-success",
      "error": "humane-libnotify-error",
      "info": "humane-libnotify-info"
    }
  },
  "google-analytics": {
    "tracking-id": "",
      "config": {
        "logging": {
          "enabled": true
        },
        "pageTracking": {
          "enabled": false
        },
        "clickTracking": {
          "enabled": false
        }
      }
  },
  "development": {
    "auth": {
      "providers": {

      }
    }
  },
  "staging": {
    "auth": {
      "providers": {

      }
    }
  },
  "production": {
    "google-analytics": {
      "config": {
        "logging": {
          "enabled": false
        },
        "pageTracking": {
          "enabled": true
        },
        "clickTracking": {
          "enabled": true
        }
      }
    },
    "auth": {
      "providers": {

      }
    }
  }
}
